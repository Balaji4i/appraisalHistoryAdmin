<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="testtt">
    <default-activity>Search</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="Search">
      <page>/fragment/Search.jsff</page>
    </view>
    <view id="t">
      <page>/fragment/t.jsff</page>
    </view>
    <control-flow-rule id="__3">
      <from-activity-id>Search</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>tosearch</from-outcome>
        <to-activity-id>t</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
