    apprisalhishdr_eo.overall_rating,                          
    apprisalhishdr_eo.person_id,                          
    apprisalhishdr_eo.suggested_final_rating,                          
    apprisalhishdr_eo.plan_name,                          
    apprisalhishdr_eo.appraisee_name,                                       
    apprisalhishdr_eo.employee_number,                          
    apprisalhishdr_eo.employee_type,                          
    apprisalhishdr_eo.historical_type,                          
    apprisalhishdr_eo.main_appraiser_id,                          
    employee_v.display_name,                            
   employee_v.PERSON_NUMBER,                        
   employee_v.BUSINESS_UNIT_NAME ,       
   employee_v.EMAIL_ADDRESS       
FROM                          
    xxpms_appraisal_hist_hdr apprisalhishdr_eo,                          
    xxper_employee_v         employee_v                          
WHERE                          
    apprisalhishdr_eo.employee_number = employee_v.person_number  
    order by  apprisalhishdr_eo.appraisal_date desc]]></SQLQuery>
